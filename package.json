{
  "name": "parquedb",
  "version": "0.1.0",
  "description": "A Parquet-based database that runs in Node.js, browsers, and Cloudflare Workers",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "workerd": "./dist/index.browser.js",
      "worker": "./dist/index.browser.js",
      "browser": "./dist/index.browser.js",
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./payload": {
      "import": "./dist/integrations/payload/index.js",
      "types": "./dist/integrations/payload/index.d.ts"
    },
    "./sql": {
      "import": "./dist/integrations/sql/index.js",
      "types": "./dist/integrations/sql/index.d.ts"
    },
    "./studio": {
      "import": "./dist/studio/index.js",
      "types": "./dist/studio/index.d.ts"
    },
    "./hono": {
      "import": "./dist/integrations/hono/index.js",
      "types": "./dist/integrations/hono/index.d.ts"
    },
    "./config": {
      "workerd": "./dist/config/index.browser.js",
      "worker": "./dist/config/index.browser.js",
      "browser": "./dist/config/index.browser.js",
      "import": "./dist/config/index.js",
      "types": "./dist/config/index.d.ts"
    },
    "./mcp": {
      "import": "./dist/integrations/mcp/index.js",
      "types": "./dist/integrations/mcp/index.d.ts"
    },
    "./search": {
      "import": "./dist/search/index.js",
      "types": "./dist/search/index.d.ts"
    },
    "./express": {
      "import": "./dist/integrations/express/index.js",
      "types": "./dist/integrations/express/index.d.ts"
    },
    "./fastify": {
      "import": "./dist/integrations/fastify/index.js",
      "types": "./dist/integrations/fastify/index.d.ts"
    },
    "./auth": {
      "workerd": "./dist/config/auth.browser.js",
      "worker": "./dist/config/auth.browser.js",
      "browser": "./dist/config/auth.browser.js",
      "import": "./dist/config/auth.js",
      "types": "./dist/config/auth.d.ts"
    },
    "./sync": {
      "import": "./dist/sync/index.js",
      "types": "./dist/sync/index.d.ts"
    }
  },
  "browser": {
    "./dist/config/auth.js": "./dist/config/auth.browser.js",
    "./dist/config/env.js": "./dist/config/env.browser.js",
    "./dist/config/index.js": "./dist/config/index.browser.js",
    "./dist/migration/index.js": "./dist/migration/index.browser.js",
    "./dist/index.js": "./dist/index.browser.js"
  },
  "peerDependencies": {
    "@modelcontextprotocol/sdk": "^1.0.0",
    "express": "^4.0.0 || ^5.0.0",
    "fastify": "^4.0.0 || ^5.0.0",
    "hono": "^4.0.0",
    "payload": "^3.0.0"
  },
  "peerDependenciesMeta": {
    "payload": {
      "optional": true
    },
    "hono": {
      "optional": true
    },
    "@modelcontextprotocol/sdk": {
      "optional": true
    },
    "express": {
      "optional": true
    },
    "fastify": {
      "optional": true
    }
  },
  "bin": {
    "parquedb": "./dist/cli-bundle.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "postinstall": "node scripts/patch-hyparquet.js",
    "build": "tsc && npx tsx scripts/fix-esm-imports.ts && pnpm build:cli",
    "build:cli": "tsx scripts/build-cli.ts",
    "test": "vitest",
    "test:unit": "vitest --project 'node:unit'",
    "test:integration": "vitest --project 'node:integration'",
    "test:e2e": "vitest --project 'node:e2e'",
    "test:e2e:workers": "vitest --workspace vitest.workspace.ts --project 'e2e'",
    "test:e2e:integration": "vitest --workspace vitest.workspace.ts --project 'e2e:integration'",
    "test:e2e:bench": "vitest bench --workspace vitest.workspace.ts --project 'e2e:bench'",
    "test:browser": "vitest --project 'browser:*'",
    "test:workers": "vitest --project 'workers:*'",
    "test:node": "vitest --project 'node:*'",
    "test:all": "vitest run",
    "test:ci": "vitest run --project 'node:*'",
    "bench": "vitest bench",
    "bench:crud": "vitest bench tests/benchmarks/crud.bench.ts",
    "bench:queries": "vitest bench tests/benchmarks/queries.bench.ts",
    "bench:relationships": "vitest bench tests/benchmarks/relationships.bench.ts",
    "bench:parquet": "vitest bench tests/benchmarks/parquet.bench.ts",
    "bench:examples": "vitest bench tests/benchmarks/examples.bench.ts",
    "bench:vector": "vitest bench tests/benchmarks/vector-search.bench.ts",
    "benchmark": "tsx scripts/benchmark.mjs",
    "typecheck": "tsc --noEmit",
    "check:datasets": "node scripts/check-datasets.mjs",
    "load:onet": "tsx scripts/load-data.ts --dataset onet --output ./data/onet",
    "load:imdb": "tsx scripts/load-data.ts --dataset imdb --output ./data/imdb",
    "load:wiktionary": "tsx scripts/load-data.ts --dataset wiktionary --output ./data/wiktionary",
    "load:unspsc": "tsx scripts/load-data.ts --dataset unspsc --output ./data/unspsc",
    "load:wikidata": "tsx scripts/load-data.ts --dataset wikidata --output ./data/wikidata",
    "load:commoncrawl": "tsx scripts/load-data.ts --dataset commoncrawl --output ./data/commoncrawl",
    "load:all": "tsx scripts/load-all.ts",
    "generate:wikidata": "tsx scripts/generate-wikidata-sample.ts --output ./data/wikidata",
    "load-test": "tsx scripts/load-test-compaction.ts",
    "load-test:a": "tsx scripts/load-test-compaction.ts --scenario=A",
    "load-test:b": "tsx scripts/load-test-compaction.ts --scenario=B",
    "load-test:c": "tsx scripts/load-test-compaction.ts --scenario=C",
    "load-test:d": "tsx scripts/load-test-compaction.ts --scenario=D"
  },
  "dependencies": {
    "@dotdo/cli": "^0.2.5",
    "@dotdo/iceberg": "^0.2.0",
    "@graphdl/core": "^0.3.0",
    "@icetype/core": "^0.3.0",
    "@icetype/iceberg": "^0.3.0",
    "@types/pluralize": "^0.0.33",
    "fflate": "^0.8.2",
    "fzstd": "^0.1.1",
    "hyparquet": "github:dot-do/hyparquet#perf/predicate-pushdown-fixes",
    "hyparquet-compressors": "^1.1.1",
    "hyparquet-writer": "github:dot-do/hyparquet-writer#variant-shredding",
    "jose": "^6.1.3",
    "pluralize": "^8.0.0",
    "snappyjs": "^0.7.0",
    "sqids": "^0.3.0"
  },
  "optionalDependencies": {
    "@aws-sdk/client-s3": "^3.980.0",
    "oauth.do": "^0.2.6"
  },
  "devDependencies": {
    "@cloudflare/vitest-pool-workers": "^0.12.8",
    "@cloudflare/workers-types": "^4.20241230.0",
    "@modelcontextprotocol/sdk": "^1.25.3",
    "@types/node": "^25.1.0",
    "@types/react": "^19.2.10",
    "@types/react-dom": "^19.2.3",
    "@vitest/browser": "^3.0.4",
    "@vitest/coverage-v8": "^3.2.4",
    "ai-database": "^2.1.3",
    "csv-parse": "^6.1.0",
    "dotenv": "^17.2.3",
    "fast-check": "^4.5.3",
    "hono": "^4.11.7",
    "lz4": "^0.6.5",
    "playwright": "^1.49.1",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "tsx": "^4.7.0",
    "typescript": "^5.7.2",
    "vitest": "^3.0.4",
    "wrangler": "^3.99.0",
    "yaml": "^2.8.2",
    "zod": "^3.25.0"
  },
  "keywords": [
    "parquet",
    "database",
    "cloudflare-workers",
    "browser",
    "nodejs"
  ],
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  }
}
